{
	"sap.app": {
		"_version": "1.3.0",
		"id": "who.corona",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{applicationTitle}}",
		"description": "{{applicationDescription}}",
		"ach": "WHO-CORONA"
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.44.11",
			"libs": {
				"sap.m": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {},
				"uri": "i18n/i18n.properties",
				"preload": true,
				"async": true
			}
		},
		"rootView": {
			"viewName": "basim.view.App",
			"type": "XML",
			"id": "idRoot"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "basim.view",
				"controlId": "idApp"
			},
			"routes": [{
				"pattern": "",
				"name": "Master",
				"target": ["masterView"]
			}, {
				"pattern": "country/{myId}",
				"name": "Detail",
				"target": ["masterView", "detailView"]
			}],
			"targets": {
				"masterView": {
					"viewName": "Master",
					"viewType": "XML",
					"controlAggregation": "masterPages"
				},
				"detailView": {
					"viewName": "Detail",
					"viewType": "XML",
					"controlAggregation": "detailPages"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize"],
		"icons": {
			"icon": "sap-icon://globe"
		}
	},
	"_version": "1.9.0"
}